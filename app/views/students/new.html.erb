<h3>Student Form</h3>

<%= form_tag new_student_path do %>

<form action="<%= new_student_path %>" method="POST">
    <label>First Name:</label>
    <input type="text" id="first_name" name="student[first_name]">
    <br>
    <label>Last Name: </label>
    <input type="text" id="last_name" name="student[last_name]">

    <%= submit_tag "Submit Student" %>

<% end %>

</form>

<% params.inspect %>